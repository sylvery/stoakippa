<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="App\Entity\Contract" table="contract">
    <indexes>
      <index name="IDX_E98F28599749932E" columns="employee_id_id"/>
    </indexes>
    <unique-constraints>
      <unique-constraint name="UNIQ_E98F2859C2CC7ADF" columns="employee_code"/>
    </unique-constraints>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="employer" type="string" column="employer" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="employeeType" type="string" column="employee_type" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="designation" type="string" column="designation" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="station" type="string" column="station" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="programme" type="string" column="programme" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="startDate" type="date" column="start_date" nullable="false"/>
    <field name="endDate" type="date" column="end_date" nullable="false"/>
    <field name="positionMt" type="string" column="position_mt" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="positionMj" type="string" column="position_mj" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="positionSg" type="string" column="position_sg" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="competencyCategory" type="string" column="competency_category" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="employeeCode" type="string" column="employee_code" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="reportsTo" type="string" column="reports_to" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-one field="employee" target-entity="Employee" fetch="LAZY">
      <join-columns>
        <join-column name="employee_id_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
