<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="App\Entity\Empleave" table="empleave">
    <indexes>
      <index name="IDX_41F8269F9749932E" columns="employee_id_id"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="leaveType" type="string" column="leave_type" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="startDate" type="date" column="start_date" nullable="false"/>
    <field name="endDate" type="date" column="end_date" nullable="false"/>
    <field name="comments" type="string" column="comments" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="statusRecommended" type="string" column="status_recommended" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="statusApproved" type="string" column="status_approved" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="recommendBy" type="string" column="recommend_by" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="approvedBy" type="string" column="approved_by" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-one field="employee" target-entity="Employee" fetch="LAZY">
      <join-columns>
        <join-column name="employee_id_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
