<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="App\Entity\Employee" table="employee">
    <unique-constraints>
      <unique-constraint name="UNIQ_5D9F75A1F85E0677" columns="username"/>
    </unique-constraints>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="username" type="string" column="username" length="180" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="roles" type="json" column="roles" nullable="false"/>
    <field name="password" type="string" column="password" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="title" type="string" column="title" length="128" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="firstName" type="string" column="first_name" length="128" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="lastName" type="string" column="last_name" length="128" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="gender" type="string" column="gender" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="maritalStatus" type="string" column="marital_status" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="dob" type="date" column="dob" nullable="false"/>
    <field name="citizenship" type="string" column="citizenship" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="province" type="string" column="province" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="village" type="string" column="village" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
